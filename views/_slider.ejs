<div class="col-md-4">

  <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title">课程信息</h3>
    </div>
    <div class="panel-body">
      <h4>{{ courseInfo.title }}</h4>
      <p>讲师: <span>{{ courseInfo.teacher }}</span></p>
      <div class="item">
        <img class="img-circle" v-bind:src="courseInfo.teacherAvatar" style="width: 120px;"></img>
      </div>
      <p class="subtitle"></p>
      <nav id="main-nav">

      </nav>
      <nav id="sub-nav">
        <div class="social">
          <a class="github" href="https://github.com/xugy0926" title="GitHub"><i class="fa fa-github fa-2x"></i></a>
        </div>
      </nav>
    </div>
  </div>

  <div class="panel panel-default" id="about-me">
    <div class="panel-heading">
      <h3 class="panel-title">小组成员</h3>
    </div>
    <div class="panel-body" v-for="item in teamMember">
      <div class="item">
        <img class="img-circle" v-bind:src="item.avatar" alt="..." style="width: 120px; height: 120px;">
      </div>
      <h4>{{ item.name }}</h4>
      <p class="subtitle"></p>
      <nav id="main-nav">
        <section class="aboutme">
          <p>{{ item.words }}</p>
        </section>
      </nav>
      <nav id="sub-nav">
        <div class="social">
          <a class="github" :href="item.github" title="Github"><i class="fa fa-github fa-2x"></i></a>
        </div>
      </nav>
    </div>
  </div>

  <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title">课程项目</h3>
    </div>
    <div class="panel-body">
      <li class="list-group-item">
        <a href="https://xugy0926.github.io/getting-started-with-javascript/">
          <i class="fa fa-github" aria-hidden="true"></i> getting-started-with-javascript
      </a>
      </li>
      <li class="list-group-item">
        <a href="https://github.com/xugy0926/words-from-the-heart">
          <i class="fa fa-github" aria-hidden="true"></i> words-from-the-heart
      </a>
      </li>
    </div>
  </div>

  <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title">GIT & GITHUB</h3>
    </div>
    <div class="panel-body">
      <li class="list-group-item" v-for="item in gitTopics">
        <a :href="item.url">{{ item.title }}</a>
      </li>
    </div>
  </div>

  <!-- <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title">PPT</h3>
    </div>
    <div class="panel-body">
      <li class="list-group-item" v-for="item in ppt">
        <a :href="item.url">{{ item.title }}</a>
      </li>
    </div>
  </div> -->

  <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title">课程的技术文档</h3>
    </div>
    <div class="panel-body">
      <li class="list-group-item" v-for="item in topics">
        <a :href="item.url">{{ item.title }}</a>
      </li>
    </div>
  </div>

</div>
